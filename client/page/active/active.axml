<view>
    <!-- 遮罩层 -->
    <view class="{{cover}}"></view>
    <!-- 顶部导航 -->
    <navigation slot="navigation" subPageName="{{subPageName}}"></navigation>
    <view class="top-frame">
        <view class="top-box-1">
            <view class="img">
                <image class="image_state {{image_active.indexOf(1) >= 0 ? '' : 'image_active'}}" mode="scaleToFill"
                    src="../../images/config/1.png"></image>
            </view>
            <view class="msg">活动信息</view>
            <view class="top_hengxian"></view>
        </view>
        <view class="top-box-1">
            <view class="img">
                <image class="image_state {{image_active.indexOf(2) >= 0 ? '' : 'image_active'}}" mode="scaleToFill"
                    src="../../images/config/2.png"></image>
            </view>
            <view class="msg">活动配置</view>
            <view class="top_hengxian"></view>
        </view>
        <view class="top-box-1">
            <view class="img">
                <image class="image_state {{image_active.indexOf(3) >= 0 ? '' : 'image_active'}}" mode="scaleToFill"
                    src="../../images/config/3.png"></image>
            </view>
            <view class="msg">投放推广</view>
        </view>
    </view>
    <!-- 活动信息 -->
    <view class="main-frame-rank {{activePid == 1 ? '' : 'frame-none' }}">
        <view class="step-frame_1_new">
            <view class="left-check-frame">
                <image mode="scaleToFill" class="rank-left-image" src="{{atmo_main}}"></image>
            </view>
        </view>
        <view class="config-frame_1_new">
            <view class="active_name_frame">
                <view class="active_name">
                    <view class="active_name_1">
                        基本信息
                    </view>
                    <view class="active_name_name">
                        活动名称:
                        <input style="margin-left:10px;width:220px" placeholder="请输入要配置的活动名称"
                            value="{{active.activeName}}" data-type="activeName" onChange="globalData"></input>
                    </view>
                    <view class="active_name_name">
                        开始时间:
                        <date-picker style="width:220px" class="block" showTime="true" value="{{active.sTime}}"
                            data-type="sTime" onChange="checkActiveTime" />
                    </view>
                    <view class="active_name_name">
                        结束时间:
                        <date-picker style="width:220px" class="block" showTime="true" value="{{active.eTime}}"
                            data-type="eTime" onChange="checkActiveTime" />
                    </view>
                </view>
            </view>
            <view class="share_frame">
                <view class="share-box-frame">
                    <view class="share-box-1">
                        <view class="share-title">分享图片:</view>
                        <view class="share-frame">
                            <view class="share-hover-frame" onTap="uploadImage"></view>
                            <image class="shareImage" mode="scaleToFill"
                                src="{{active.basisConfig.shareConfig.shareURL}}"></image>
                        </view>
                        <view class="gift_msg_1">淘宝分享图,请上传420PX*420PX的png格式图片,图片大小不超过200kb, 如未上传则使用默认分享图</view>
                    </view>
                    <view class="share-box-2">
                        <view class="share-title">分享标题:</view>
                        <view>
                            <input value="{{active.basisConfig.shareConfig.shareTitle}}" data-type="title"
                                onChange="updateShareInfo"></input>
                        </view>
                    </view>
                    <view class="share-box-3">
                        <view class="share-title">淘口令:</view>
                        <view>
                            <input value="{{active.basisConfig.shareConfig.shareDesc}}" data-type="desc"
                                onChange="updateShareInfo"></input>
                        </view>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <!-- 基本配置 -->
    <view class="main-frame-system {{activePid == 2 ? '' : 'frame-none' }}">
        <view class="step-frame_1">
            <view class="left-check-frame">
                <view class="check_box_frame" data-typeId="{{activeTypeList.daily.typeId}}"
                    data-config="{{activeTypeList.daily.config}}" onTap="checkActiveEvent">
                    <image class="{{check_active_type == activeTypeList.daily.typeId ? 'check_images' : 'cover_none'}}"
                        mode="scaleToFill" src="../../images/config/check-box.png"></image>
                    <view class="msg_1">{{activeTypeList.daily.title}}</view>
                    <view class="msg_2">{{activeTypeList.daily.msg}}</view>
                    <view class="msg_3">
                        <view class="msg_3_left">{{activeTypeList.daily.desc}}</view>
                    </view>
                </view>
            </view>
        </view>
        <view class="config-frame_1">
            <view class="ship_frame">
                <view class="ship_frame_1">
                    促销配置:
                </view>
                <view class="ship_frame_top">
                    <view style="line-height:35px;color:#666666;font-size:15px;">活动商品:</view>
                    <view class="check_shipinfo" data-configType="promotion" onTap="checkShipData">配置促销商品</view>
                    <view class="ship_msg">需要配置促销商品, 建议可以全选商品,用户浏览或购买选中商品将会完成该任务.</view>
                </view>
                <view style="font-size: 13px;font-weight:bold;margin-top: 2px;color: #ff812a;margin-left:103px">
                    (已选择{{active.basisConfig.promotionList.length}}件商品)</view>
                <view class="fenxi_frame">
                    <view class="fenxi_frame_1">会员运营开关:</view>
                    <view class="fenxi_frame_2">
                        <checkbox class="block" checked="{{active.basisConfig.isVipSystem}}" data-type="isVipSystem"
                            onChange="checkSwitchEvent"></checkbox>
                        <text class="fenxi_frame_2_msg">如果店铺没有会员体系，则不需要</text>
                    </view>
                </view>
            </view>
            <view class="ship_frame" style="margin-top:15px">
                <view class="ship_frame_1">
                    奖品配置:
                </view>
                <view class="reward_frame">
                    <view class="reward_frame_1">活动奖励:</view>
                    <view class="reward_jiantou">＞＞</view>
                    <view class="reward_frame_3">
                        <block a:if="{{active.integralRewardConfig.rewardList[0].title == undefined}}">
                            <view class="reward_frame_3_msg">奖品未配置</view>
                        </block>
                        <block a:else=>
                            <view class="reward_frame_2_msg">奖品已配置</view>
                        </block>
                        <view class="reward_frame_3_btn" data-opType="open" onTap="openIntegralRewardBox">
                            配置积分奖励
                        </view>
                    </view>
                    <view class="reward_jiantou"> ＜＜</view>
                    <view class="reward_jiantou">＞＞</view>
                    <view class="reward_frame_3">
                        <block a:if="{{active.mergeRewardConfig[0].title == undefined}}">
                            <view class="reward_frame_3_msg">奖品未配置</view>
                        </block>
                        <block a:else=>
                            <view class="reward_frame_2_msg">奖品已配置</view>
                        </block>
                        <view class="reward_frame_3_btn" data-opType="open" onTap="openMergeRewardBox">
                            配置合成奖励
                        </view>
                    </view>
                    <view class="reward_jiantou"> ＜＜</view>
                </view>
            </view>
            <view class="ship_frame" style="margin-top:15px">
                <view class="ship_frame_1">弹窗配置:</view>
                <view class="reward_frame">
                    <view class="reward_frame_3">
                        <block
                            a:if="{{!active.bouncedConfig.luckyBag.title }}">
                            <view class="reward_frame_3_msg">未配置</view>
                        </block>
                        <block a:else=>
                            <view class="reward_frame_2_msg">已配置</view>
                        </block>
                        <view class="reward_frame_3_btn" data-opType="open" onTap="openBouncedBox">配置游戏弹窗</view>
                    </view>
                </view>
            </view>
            <view class="ship_frame" style="margin-top:15px">
                <view class="ship_frame_1">
                    奖励次数任务配置:
                </view>
                <view class="mission_frame">
                    <view class="mission_frame_2">
                        <view class="mission_frame_2_1">任务标题</view>
                        <view class="mission_frame_2_2">每日领奖上限</view>
                        <view class="mission_frame_2_3">次数、积分</view>
                        <view class="mission_frame_2_4">领取间隔</view>
                    </view>
                    <view class="hengxian"></view>
                    <view class="mission_frame_3">
                        <view class="mission_frame_3_1">
                            <block a:for="{{activeTypeList.daily.config.missionConfig}}" a:for-item="item">
                                <block a:if="{{item.bTitle != '不展示' }}">
                                    <view class="mission_frame_3_1_1">{{item.bTitle}}
                                        {{item.bTitle == '加入会员' || item.bTitle == '会员福利' ? '(需打开会员开关)' : '' }}</view>
                                </block>
                            </block>
                        </view>
                        <view class="mission_frame_3_2">
                            <block a:for="{{activeTypeList.daily.config.missionConfig}}" a:for-item="item">
                                <view class="mission_frame_3_2_1">
                                    <block a:if="{{item.bTitle != '不展示' }}">
                                        <view class="mission_frame_3_2_1_1">
                                            <block
                                                a:if="{{item.bTitle == '关注店铺'  || item.bTitle == '加入会员' || item.bTitle == '邀请1名好友' || item.bTitle == '购买店铺商品'}}">
                                            </block>
                                            <block a:else=>
                                                <input class="mission_frame_3_2_input_frame" data-item="{{item}}"
                                                    data-type="limit" data-activeType={{active.activeType}}
                                                    onChange="editMissionConfig"
                                                    value="{{item.limit >=0 ? item.limit : '' }}"></input>
                                                <view style="margin-left:10px;">次</view>
                                            </block>
                                        </view>
                                    </block>
                                </view>
                            </block>
                        </view>
                        <view class="mission_frame_3_3">
                            <block a:for="{{activeTypeList.daily.config.missionConfig}}" a:for-item="item">
                                <view class="mission_frame_3_2_1">
                                    <block a:if="{{item.bTitle != '不展示' }}">
                                        <block a:if="{{item.bTitle === '购买店铺商品'}}">
                                            <view style="margin-left:-59px;margin-right:5px">商品价格x</view>
                                            <input type="text" class="mission_frame_3_2_input_frame"
                                                data-item="{{item}}" data-type="rewardCount_1"
                                                data-activeType={{active.activeType}} onChange="editMissionConfig"
                                                value="{{item.rewardCount_1 >=0 ? item.rewardCount_1 : '' }}"></input>
                                            <view style="margin-left:10px;">次数</view>
                                            <view style="margin-left:5px;margin-right:5px">商品价格x</view>
                                            <input type="text" class="mission_frame_3_2_input_frame"
                                                data-item="{{item}}" data-type="rewardCount_2"
                                                data-activeType={{active.activeType}} onChange="editMissionConfig"
                                                value="{{item.rewardCount_2 >= 0 ? item.rewardCount_2: '' }}"></input>
                                            <view style="margin-left:10px;">积分</view>
                                        </block>
                                        <block a:else=>
                                            <input type="text" style="margin-left:5px"
                                                class="mission_frame_3_2_input_frame" data-item="{{item}}"
                                                data-type="rewardCount_1" data-activeType={{active.activeType}}
                                                onChange="editMissionConfig"
                                                value="{{item.rewardCount_1 >=0 ? item.rewardCount_1 : '' }}"></input>
                                            <view style="margin-left:10px;">次数</view>
                                            <input type="text" class="mission_frame_3_2_input_frame"
                                                data-item="{{item}}" data-type="rewardCount_2"
                                                data-activeType={{active.activeType}} onChange="editMissionConfig"
                                                value="{{item.rewardCount_2 >=0 ? item.rewardCount_2 : ''}}"></input>
                                            <view style="margin-left:10px;">积分</view>
                                        </block>
                                    </block>
                                </view>
                            </block>
                        </view>
                        <view class="mission_frame_3_4">
                            <block a:for="{{activeTypeList.daily.config.missionConfig}}" a:for-item="item">
                                <view class="mission_frame_3_2_1">
                                    <block a:if="{{item.bTitle != '不展示' }}">
                                        <view class="mission_frame_3_2_1_1">
                                            <block
                                                a:if="{{item.bTitle == '关注店铺' || item.bTitle == '加入会员' || item.bTitle == '邀请好友1名好友' || item.bTitle == '购买店铺商品' || item.bTitle == '浏览商品页10s' || item.bTitle == '商品收藏' }}">
                                            </block>
                                            <block a:else=>
                                                <input type="text" class="mission_frame_3_2_input_frame"
                                                    value="{{item.need}}" data-item="{{item}}" data-type="need"
                                                    data-activeType={{active.activeType}}
                                                    onChange="editMissionConfig"></input>
                                                <view style="margin-left:10px;">秒</view>
                                            </block>
                                        </view>
                                    </block>
                                </view>
                            </block>
                        </view>
                    </view>
                </view>
            </view>
            <view class="ship_frame" style="margin-top:15px">
                <view class="ship_frame_1">赚取更多积分任务配置:</view>
                <view class="mission_frame">
                    <view class="mission_frame_2">
                        <view class="mission_frame_2_1">任务标题</view>
                        <view class="mission_frame_2_2">每日领奖上限</view>
                        <view class="mission_frame_2_3">次数、积分</view>
                        <view class="mission_frame_2_4">领取间隔</view>
                    </view>
                    <view class="hengxian"></view>
                    <view class="mission_frame_3">
                        <view class="mission_frame_3_1">
                            <block a:for="{{activeTypeList.daily.config.missionConfig}}" a:for-item="item">
                                <block a:if="{{item.bTitle != '不展示' && item.mType == 'more' }}">
                                    <view class="mission_frame_3_1_1">{{item.bTitle}}
                                        {{item.bTitle == '加入会员' || item.bTitle == '会员福利' ? '(需打开会员开关)' : '' }}</view>
                                </block>
                            </block>
                        </view>
                        <view class="mission_frame_3_2">
                            <block a:for="{{activeTypeList.daily.config.missionConfig}}" a:for-item="item">
                                <view class="mission_frame_3_2_1">
                                    <block a:if="{{item.bTitle != '不展示' && item.mType == 'more' }}">
                                        <view class="mission_frame_3_2_1_1">
                                            <block
                                                a:if="{{item.bTitle == '关注店铺'  || item.bTitle == '加入会员' || item.bTitle == '邀请1名好友' || item.bTitle == '购买店铺商品'}}">
                                            </block>
                                            <block a:else=>
                                                <input disabled="true" class="mission_frame_3_2_input_frame"
                                                    data-item="{{item}}" data-type="limit"
                                                    data-activeType="{{active.activeType}}" onChange="editMissionConfig"
                                                    value="{{item.limit >=0 ? item.limit : '' }}"></input>
                                                <view style="margin-left:10px;">次</view>
                                            </block>
                                        </view>
                                    </block>
                                </view>
                            </block>
                        </view>
                        <view class="mission_frame_3_3">
                            <block a:for="{{activeTypeList.daily.config.missionConfig}}" a:for-item="item">
                                <view class="mission_frame_3_2_1">
                                    <block a:if="{{item.bTitle != '不展示' && item.mType == 'more' }}">
                                        <block a:if="{{item.bTitle === '购买店铺商品'}}">
                                            <view style="margin-left:-59px;margin-right:5px">商品价格x</view>
                                            <input disabled="true" type="text" class="mission_frame_3_2_input_frame"
                                                data-item="{{item}}" data-type="rewardCount_1"
                                                data-activeType="{{active.activeType}}" onChange="editMissionConfig"
                                                value="{{item.rewardCount_1 >=0 ? item.rewardCount_1 : '' }}"></input>
                                            <view style="margin-left:10px;">次数</view>
                                            <view style="margin-left:5px;margin-right:5px">商品价格x</view>
                                            <input disabled="true" type="text" class="mission_frame_3_2_input_frame"
                                                data-item="{{item}}" data-type="rewardCount_2"
                                                data-activeType="{{active.activeType}}" onChange="editMissionConfig"
                                                value="{{item.rewardCount_2 >= 0 ? item.rewardCount_2: '' }}"></input>
                                            <view style="margin-left:10px;">积分</view>
                                        </block>
                                        <block a:else=>
                                            <input disabled="true" type="text" style="margin-left:5px"
                                                class="mission_frame_3_2_input_frame" data-item="{{item}}"
                                                data-type="rewardCount_1" data-activeType="{{active.activeType}}"
                                                onChange="editMissionConfig"
                                                value="{{item.rewardCount_1 >=0 ? item.rewardCount_1 : '' }}"></input>
                                            <view style="margin-left:10px;">次数</view>
                                            <input disabled="true" type="text" class="mission_frame_3_2_input_frame"
                                                data-item="{{item}}" data-type="rewardCount_2"
                                                data-activeType="{{active.activeType}}" onChange="editMissionConfig"
                                                value="{{item.rewardCount_2 >=0 ? item.rewardCount_2 : ''}}"></input>
                                            <view style="margin-left:10px;">积分</view>
                                        </block>
                                    </block>
                                </view>
                            </block>
                        </view>
                        <view class="mission_frame_3_4">
                            <block a:for="{{activeTypeList.daily.config.missionConfig}}" a:for-item="item">
                                <view class="mission_frame_3_2_1">
                                    <block a:if="{{item.bTitle != '不展示' && item.mType == 'more' }}">
                                        <view class="mission_frame_3_2_1_1">
                                            <block
                                                a:if="{{item.bTitle == '关注店铺' || item.bTitle == '加入会员' || item.bTitle == '邀请好友1名好友' || item.bTitle == '购买店铺商品' || item.bTitle == '浏览商品页10s' || item.bTitle == '商品收藏' }}">
                                            </block>
                                            <block a:else=>
                                                <input disabled="true" type="text" class="mission_frame_3_2_input_frame"
                                                    value="{{item.need}}" data-item="{{item}}" data-type="need"
                                                    data-activeType="{{active.activeType}}"
                                                    onChange="editMissionConfig"></input>
                                                <view style="margin-left:10px;">秒</view>
                                            </block>
                                        </view>
                                    </block>
                                </view>
                            </block>
                        </view>
                    </view>
                </view>
            </view>
            <view class="ship_frame" style="margin-top:15px">
                <view class="ship_frame_1">
                    规则设置:
                </view>
                <view class="desc_frame">
                    <view class="desc_frame_title">
                        活动规则
                    </view>
                    <view class="desc_frame_desc">
                        <textarea value="{{active.rule}}" rows="15" onChange="updateDesc" hasLimitHint="true"
                            maxLength="1000"></textarea>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <!-- 投放 -->
    <view class="main-frame-put">
        <view class="put-icon-frame">
            <view class="{{activePid == 3 ? '' : 'frame-none' }}">
                <image class="put-icon-image" mode="scaleToFill" src="../../images/config/putIcon.png"></image>
            </view>
            <view class="{{activePid == 3 ? '' : 'frame-none' }}" style="width:100%;text-align: center;font-size:13px">
                那么多好位置开始推广投放吧</view>
            <view class="qrcode-frame">
                <view class="qrcode-image {{activePid == 3 ? '' : 'visibility' }}">
                    <view>请扫描以下二维码进入活动</view>
                    <canvas style="width: 150px;height: 150px;margin-left: 20px;" id="mycanvasDetail"></canvas>
                </view>
                <view class="qrcode-center-frame {{activePid == 3 ? '' : 'frame-none' }}">
                    <view style="color:#FF8E56">活动链接</view>
                    <view class="qrcode-input-frame">
                        <input value="{{c_tb_url + '&query=' + 'activeId=' + activeId}}"></input>
                        <view class="copy-btn" data-activeUrl="{{c_tb_url + '&query=' + 'activeId=' + activeId}}"
                            onTap="setClipboard">复制链接</view>
                    </view>
                    <view style="font-size: 13px;margin-top: 6px;">
                        关闭此界面后,在
                        <text style="color:#579FFF" data-type="configlist" onTap="jump">活动列表</text>
                        还可以找到!!!
                    </view>
                </view>
            </view>
            <view class="down-box-frame {{activePid == 3 ? '' : 'frame-none' }}">
                <view class="tf-box-frame">
                    <view class="tf-box-left-frame">
                        <image mode="scaleToFill" src="../../images/config/zhibo1.png"></image>
                    </view>
                    <view class="tf-box-right-frame">
                        <view class="tf-btn-1-frame" data-type="video" data-img="config/zhibo.png" onTap="tf_btnEvent">
                            去投放</view>
                        <view class="tf-btn-3-frame" data-type="video_Tutorials" onTap="tf_jump">查看教程</view>
                    </view>
                </view>
                <view class="tf-box-frame">
                    <view class="tf-box-left-frame">
                        <image mode="scaleToFill" src="../../images/config/zhibo2.png"></image>
                    </view>
                    <view class="tf-box-right-frame">
                        <view class="tf-btn-1-frame" data-type="toufang" data-img="config/zhibo.png"
                            onTap="tf_btnEvent">去投放</view>
                        <view class="tf-btn-3-frame" data-type="video_Tutorials" onTap="tf_jump">查看教程</view>
                    </view>
                </view>
                <view class="tf-box-frame">
                    <view class="tf-box-left-frame">
                        <image mode="scaleToFill" src="../../images/config/shouye1.png"></image>
                    </view>
                    <view class="tf-box-right-frame">
                        <view class="tf-btn-1-frame" data-type="tb_wangwang" data-img="config/shouye.png"
                            onTap="tf_btnEvent">去投放</view>
                        <view class="tf-btn-3-frame" data-type="taobao_main" onTap="tf_jump">查看教程</view>
                    </view>
                </view>
                <view class="tf-box-frame">
                    <view class="tf-box-left-frame">
                        <image mode="scaleToFill" src="../../images/config/xiangqing1.png"></image>
                    </view>
                    <view class="tf-box-right-frame">
                        <view class="tf-btn-1-frame" data-type="tb_wangwang" data-img="config/xiangqing.png"
                            onTap="tf_btnEvent">去投放</view>
                        <view class="tf-btn-3-frame" data-type="taobao_detail" onTap="tf_jump">查看教程</view>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <!-- 图片弹窗 -->
    <view class="{{tf_img_frame}}">
        <view class="top-frame-images">
            <view class="right-close" onTap="close">
                <image mode="scaleToFill" src="../../images/configlist/close.png"></image>
            </view>
        </view>
        <image class="{{imgTmp == 'config/xiangqing.png' ? 'image_4' : 'image_3'}}" mode="scaleToFill" onTap="close"
            src="{{tf_img_url}}"></image>
        <view data-type="{{jumpType}}" onTap="tf_jump" class="openwangwang">打开旺铺</view>
    </view>
    <!-- 下一步 -->
    <view class="footer_rate_frame {{activePid == 3 ? 'frame-none' : ''}}">
        <view class="footer_box_frame">
            <view class="left_btn_frame_up {{activePid > 1 ?  '' : 'visibility'}}" data-type="stepUp"
