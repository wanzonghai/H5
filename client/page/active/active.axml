                        <input disabled="false" class="ship-detail-box-1"
                            value="{{active.bouncedConfig.luckyBag.title ? active.bouncedConfig.luckyBag.title : 'XXXXXX' }}"></input>
                    </view>
                    <view class="ship-center-title" style="margin-top:10px;">
                        商品金额:
                        <input disabled="false" class="ship-detail-box-1"
                            value="{{active.bouncedConfig.luckyBag.price ? active.bouncedConfig.luckyBag.price : '00.00' }}元"></input>
                    </view>
                </view>
                <view class="ship-center-frame">
                    <view class="ship-center-title">
                        商品库存:
                        <input disabled="false" class="ship-detail-box-1"
                            value="{{active.bouncedConfig.luckyBag.num ? active.bouncedConfig.luckyBag.num : '' }}"></input>
                    </view>
                    <view class="ship-center-title" style="margin-top:10px;">
                        剩余库存:
                        <input disabled="false" class="ship-detail-box-1"
                            value="{{active.bouncedConfig.luckyBag.nums ? active.bouncedConfig.luckyBag.nums : '' }}"></input>
                    </view>
                </view>
            </view>
        </view>
        <view class="hengxian" style="margin-top:10px"></view>
        <scroll-view style="height:270px" trap-scroll="{{true}}" scroll-y="{{true}}" onScrollToUpper="upper"
            onScrollToLower="lower" onScroll="scroll" scroll-left="1">
            <view class="bouncedBox_1">
                <view class="bouncedBox_1_1">
                    游戏
                    <text style="color:red">开始</text>
                    弹窗配置
                </view>
                <view class="bouncedBox_1_2">(658px * 900px)</view>
                <view class="bouncedBox_1_3">
                    <view class="bouncedBox_1_3_1">
                        <block a:if="{{ active.bouncedConfig.startGame.yes !== '' }}">
                            <view class="bouncedBox_1_3_1_1">
                                <image src="{{active.bouncedConfig.startGame.yes}}"></image>
                            </view>
                        </block>
                        <view class="bouncedBox_1_3_1_2">
                            <view data-type="startGame" data-state="yes" onTap="gameUploadImage"
                                class="bouncedBox_1_3_1_2_1">
                                <block a:if="{{ active.bouncedConfig.startGame.yes !== '' }}">替换图片</block>
                                <block a:else=>
                                    选择图片
                                </block>
                            </view>
                            <view class="bouncedBox_1_3_1_2_2">未购买福袋弹框</view>
                        </view>
                    </view>
                    <view class="bouncedBox_1_3_1">
                        <block a:if="{{ active.bouncedConfig.startGame.no !== '' }}">
                            <view class="bouncedBox_1_3_1_1">
                                <image src="{{active.bouncedConfig.startGame.no}}"></image>
                            </view>
                        </block>
                        <view class="bouncedBox_1_3_1_2">
                            <view data-type="startGame" data-state="no" onTap="gameUploadImage"
                                class="bouncedBox_1_3_1_2_1">
                                <block a:if="{{ active.bouncedConfig.startGame.no !== '' }}">替换图片</block>
                                <block a:else=>
                                    选择图片
                                </block>
                            </view>
                            <view class="bouncedBox_1_3_1_2_2">已购买福袋弹框</view>
                        </view>
                    </view>
                </view>
            </view>
            <view class="bouncedBox_1">
                <view class="bouncedBox_1_1">
                    游戏
                    <text style="color:red">结束</text>
                    弹窗配置
                </view>
                <view class="bouncedBox_1_2">(658px * 900px)</view>
                <view class="bouncedBox_1_3">
                    <view class="bouncedBox_1_3_1">
                        <block a:if="{{ active.bouncedConfig.endGame.yes !== '' }}">
                            <view class="bouncedBox_1_3_1_1">
                                <image src="{{active.bouncedConfig.endGame.yes}}"></image>
                            </view>
                        </block>
                        <view class="bouncedBox_1_3_1_2">
                            <view data-type="endGame" data-state="yes" onTap="gameUploadImage"
                                class="bouncedBox_1_3_1_2_1">
                                <block a:if="{{ active.bouncedConfig.endGame.yes !== '' }}">替换图片</block>
                                <block a:else=>
                                    选择图片
                                </block>
                            </view>
                            <view class="bouncedBox_1_3_1_2_2">未购买福袋弹框</view>
                        </view>
                    </view>
                    <view class="bouncedBox_1_3_1">
                        <block a:if="{{ active.bouncedConfig.endGame.no !== '' }}">
                            <view class="bouncedBox_1_3_1_1">
                                <image src="{{active.bouncedConfig.endGame.no}}"></image>
                            </view>
                        </block>
                        <view class="bouncedBox_1_3_1_2">
                            <view data-type="endGame" data-state="no" onTap="gameUploadImage"
                                class="bouncedBox_1_3_1_2_1">
                                <block a:if="{{ active.bouncedConfig.endGame.no !== '' }}">替换图片</block>
                                <block a:else=>
                                    选择图片
                                </block>
                            </view>
                            <view class="bouncedBox_1_3_1_2_2">已购买福袋弹框</view>
                        </view>
                    </view>
                </view>
            </view>
        </scroll-view>
        <view class="hengxian" style="margin-top:10px"></view>
        <view class="bouncedBox_2">
            <view class="bouncedBox_2_1" data-opType="success" onTap="openBouncedBox">确定</view>
        </view>
    </view>
</view>